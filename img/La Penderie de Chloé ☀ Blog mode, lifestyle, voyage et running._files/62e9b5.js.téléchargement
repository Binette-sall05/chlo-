jQuery( document ).ready(function( $ ) {

	//$('.skdLazy').lazy();
	$('.promo-ligne-1.cf img').lazy();
	$('#sb_instagram').lazy();
	$('.skdLazy').lazy();

	if($('.skdLazyPostImage').length > 0){
    $('.skdLazyPostImage').each(function(){
      if($(this).is('img')){
        $('.skdLazyPostImage').lazy();
      }
      else{
        $(this).find('img').lazy();
      }
    });
  }
	$('img').lazy();

  //$('.skdLazyPostImage').lazy();
  //$('.skdLazyPostImage img').lazy();

  //$('#logo-sticky img').attr('data-srcset', '/wp-content/themes/lapenderiedechloe/library/images/logo.png');

	if($('iframe').length > 0){
		$('iframe').each(function(){
			$(this).attr('src', $(this).attr('data-src'));
			$(this).removeClass('skdLazyPostImage');
		});
	}


	function balanceSearch(){
		if($('nav.sticky-menu').hasClass('is-sticky')){
			$('#search-overlay').css('top', $('nav.sticky-menu').outerHeight());
		}
		else{
			$('#search-overlay').css('top', $('.main-header').outerHeight());
		}
	}

	function balanceMenu(){
		if($('nav.sticky-menu').hasClass('is-sticky')){
			$('.sticky-menu-content').css('top', $('nav.sticky-menu').outerHeight());
		}
		else{

			$('.sticky-menu-content').css('top', $('.main-header').outerHeight()+45);
		}

		if($('.sticky-menu-content').hasClass('hidden')){
			$('#top-toggler').removeClass('active');
			$('#sticky-toggler').removeClass('active');
		}
		else{
			$('#top-toggler').addClass('active');
			$('#sticky-toggler').addClass('active');
		}
	}

  function adaptFullImage(){
    if($(window).outerWidth() > 1100){
      negmarg = ($(window).width() - $('body.single .wrap').width()) / 2;
    }
    else{
      negmarg = $('body.single .entry-content').css('padding-left').replace('px','');
    }
    $('img.full ').css('margin-left', -negmarg);
    $('img.full ').css('margin-right', -negmarg);
    $('img.full ').css('width', $(window).width());
    $('img.full ').css('max-width', 'unset');
  }

	// OPEN SEARCH
	$('.open-searchform').click(function(){
		balanceSearch();
		$('body').toggleClass('open-search');
		$('.search-form .search-field').focus();
		return false;
	});

	// CLOSE SEARCH
	$(document).click(function (e) {
		 if ($(e.target).is('#search-overlay')) {
	    	$('body').removeClass('open-search');
	        return false;
	    }
	});

	// OPEN STICKY NAV (from main)
	$('#sticky-toggler').click(function(){
		$(this).toggleClass('active');
		$('.sticky-menu-content').toggleClass('hidden');
		balanceMenu();
		return false;
	});

	// OPEN STICKY NAV (from sticky)
	$('#top-toggler').click(function(){
		$(this).toggleClass('active');
		$('.sticky-menu-content').toggleClass('hidden');
		balanceMenu();
		return false;
	});

	$(window).on('scroll', function() {
		if($('body').hasClass('open-search')){
			balanceSearch();
		}

		if($(this).scrollTop() > 0){
			$('#sticky-youtube').addClass('reduced');
		}
		else{
			$('#sticky-youtube').removeClass('reduced');
		}

		balanceMenu();
	});

	$(window).on('resize', function(){
		if($('body').hasClass('open-search')){
			balanceSearch();
		}
		balanceMenu();
    adaptFullImage();
	});

  adaptFullImage();

